<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lovelyblue.ascrossappreceivesideexample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.ReceiveActivity1"
            android:exported="true"
            android:label="@string/title_activity_recive1" >
        </activity>
        <activity
            android:name=".Activitys.ReceiveActivity2"
            android:exported="true"
            android:label="@string/title_activity_receive2" >
        </activity>
        <activity
            android:name=".Activitys.ReceiveActivity3"
            android:exported="true"
            android:label="@string/title_activity_receive3" >
        </activity>
    </application>

</manifest>
